<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sonatra_gluon.custom_asset.bootstrap_variables" synthetic="true">
            <tag name="fxp_require_asset.assetic.custom_asset" />
            <argument>sonatra-gluon/custom_variables.less</argument>
            <argument type="collection" />
            <argument>sonatra-gluon-bundle-custom-variables-filename</argument>
        </service>

        <service id="sonatra_gluon.custom_asset.bootstrap_mixins" synthetic="true">
            <tag name="fxp_require_asset.assetic.custom_asset" />
            <argument>sonatra-gluon/custom_mixins.less</argument>
            <argument type="collection" />
            <argument>sonatra-gluon-bundle-custom-mixins-filename</argument>
        </service>
    </services>
</container>
